<MainFrame>:
    orientation: 'vertical'
    BoxLayout:
        orientation: 'horizontal'
        size_hint: (1, .05)
        Button:
            id: play
            text: 'играть'
            on_press: app.start_play(self)
        Button:
            id: add
            text: 'добавить частоту'
            on_press: app.add_freq(frcon)
    BoxLayout:
        id: frcon
